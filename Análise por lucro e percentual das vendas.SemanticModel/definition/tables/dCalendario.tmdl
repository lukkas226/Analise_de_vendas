	partition dCalendario = calculated
		mode: import
		source =
				
				VAR DataInicial = DATE(2023, 01, 01) -- Defina o ano que seus dados começam
				VAR DataFinal = DATE(2025, 12, 31)   -- Defina até onde quer que o calendário vá
				RETURN
				ADDCOLUMNS(
				    CALENDAR(DataInicial, DataFinal),
				    "Ano", YEAR([Date]),
				    "Mês Num", MONTH([Date]),
				    "Mês Nome", FORMAT([Date], "MMMM"),
				    "Mês/Ano", FORMAT([Date], "MMM/YY"),
				    "Trimestre", "Trim " & QUARTER([Date]),
				    "Dia da Semana", WEEKDAY([Date]),
				    "Nome do Dia", FORMAT([Date], "DDDD")
				)

